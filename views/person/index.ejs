<!--index.ejs-->

<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Clinical Decision Support System (CDSS)</h1>
        <p class="lead">HKBU Event Management System is a web-based system for department and societies to
            broadcast
            events and activities as well as registration</p>
    </div>
</div>
<div class="row col-12">
<% persons.forEach( function(model) { %>
<div class="card col-4" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">
            Disease: Lung Cancer
        </h5>
        <h6 class="card-subtitle mb-2 text-muted">
            Patientï¼š <%= model.patientName %>
        </h6>
        <p class="card-text">
            Size Of The Original Tumour: <%= model.sizeOfTheOriginalTumour %>
        </p>
        <a href="person/detail/<%= model.id %>" class="card-link">link to detail</a>
    </div>
</div>
<% }); %>
</div>



